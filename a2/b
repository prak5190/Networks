  width: 100px;
  height: 50px;
  /* THE DIMENSION OF A SINGLE THUMB */

  cursor: pointer;
  overflow: hidden;
  background: none;
  margin: 0;
  float: left;
  -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=50)";
  /*filter: alpha(opacity=50);	*/

  -moz-opacity: 0.5;
  -khtml-opacity: 0.5;
  opacity: 0.5;
